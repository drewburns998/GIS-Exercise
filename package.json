{
  "name": "mobikit",
  "version": "1.0.0",
  "description": "Coding Exercise",
  "main": "index.jsx",
  "scripts": {
    "start": "parcel index.html",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint:check": "eslint --ignore-path .gitignore .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>src/jest.config.js"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drewburns998/mobikit.git"
  },
  "author": "Andrew Burns",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drewburns998/mobikit/issues"
  },
  "homepage": "https://github.com/drewburns998/mobikit#readme",
  "dependencies": {
    "@ag-grid-community/all-modules": "^23.2.1",
    "@ag-grid-community/react": "^23.2.1",
    "@turf/boolean-point-in-polygon": "^6.0.1",
    "@turf/turf": "^5.1.6",
    "ag-grid-community": "^23.2.1",
    "ag-grid-react": "^23.2.1",
    "babel-jest": "^26.1.0",
    "json-loader": "^0.5.7",
    "react": "^16.13.1",
    "react-csv-reader": "^3.1.0",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "turf-aggregate": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^9.1.2",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "jest-css-modules": "^2.1.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "redux-mock-store": "^1.5.4"
  }
}
